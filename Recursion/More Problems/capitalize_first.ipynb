   "source": [
    "def capitalizeFirst(lst):\n",
    "    result = []\n",
    "    if len(lst) == 0:\n",
    "        return result\n",
    "    result.append(lst[0][0].upper() + lst[0][1:])\n",
    "    return result + capitalizeFirst(lst[1:]) "
   ]
  },
